<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>



    <style name="TextStyleBoldBlack_12">
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:fontFamily">@font/opensansbold</item>
    </style>

    <style name="TextStyleSemiBoldBlack_12">
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:fontFamily">@font/opensanssemibold</item>
    </style>


    <style name="TextStyleRegularWhite_12">
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/opensansregular</item>
    </style>


    <style name="TextStyleSemiBoldBlue_12">
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:fontFamily">@font/opensanssemibold</item>
    </style>



</resources>
